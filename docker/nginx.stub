server {
    listen 80;
    server_name openapi.lead9;
    client_max_body_size 10m;

    location / {
        proxy_pass http://127.0.0.1:8107;
        proxy_set_header HOST $host;

        proxy_hide_header 'Access-Control-Allow-Origin';
        add_header 'Access-Control-Allow-Origin' '*' always;
    }
}
